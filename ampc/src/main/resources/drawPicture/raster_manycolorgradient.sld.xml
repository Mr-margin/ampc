<?xml version="1.0" encoding="ISO-8859-1"?>
<StyledLayerDescriptor version="1.0.0" 
    xsi:schemaLocation="http://www.opengis.net/sld StyledLayerDescriptor.xsd" 
    xmlns="http://www.opengis.net/sld" 
    xmlns:ogc="http://www.opengis.net/ogc" 
    xmlns:xlink="http://www.w3.org/1999/xlink" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <NamedLayer>
    <Name>Many color gradient</Name>
    <UserStyle>
      <Title>SLD Cook Book: Many color gradient</Title>
      <FeatureTypeStyle>
        <Rule>
          <RasterSymbolizer>
            <ColorMap>
              
              <ColorMapEntry color="#FDF9F3" quantity="0" opacity="0"/>
              <ColorMapEntry color="#009BFF" quantity="2" />
              <ColorMapEntry color="#02EFCC" quantity="10" />
              <ColorMapEntry color="#07C13B" quantity="15" />
              <ColorMapEntry color="#6ED209" quantity="20" />
              <ColorMapEntry color="#FFFF00" quantity="25" />
              <ColorMapEntry color="#FFC800" quantity="30" />
			  <ColorMapEntry color="#FF8200" quantity="35" />
			  <ColorMapEntry color="#FF1E00" quantity="40" />
			  <ColorMapEntry color="#B8003A" quantity="45" />
			  <ColorMapEntry color="#5C0084" quantity="50" />
			  
            </ColorMap>
          </RasterSymbolizer>
        </Rule>
      </FeatureTypeStyle>
    </UserStyle>
  </NamedLayer>
</StyledLayerDescriptor>
